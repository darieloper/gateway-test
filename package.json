{
  "name": "gateways-nodejs",
  "version": "1.0.0",
  "description": "gateways test project",
  "main": "index.js",
  "scripts": {
    "dev": "node ./index.js",
    "watch": "npx nodemon ./index.js",
    "seed": "node ./database/seed.js",
    "start:front": "cd ./front && npm run start",
    "build:front": "cd ./front && npm run build",
    "start:all": "npx concurrently \"npm:dev\" \"npm run start:front\"",
    "test": "NODE_ENV=test && npm run seed && jest tests",
    "build": "npm run build:front && NODE_ENV=prod npm run dev",
    "start": "npm run build"
  },
  "keywords": [
    "test",
    "musala",
    "gateways",
    "nodejs",
    "react",
    "mongodb"
  ],
  "author": "Dariel Ramos",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "jest": "^27.4.7",
    "mongoose": "^6.1.6",
    "supertest": "^6.2.1",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^5.5.3",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "ignore": [
      "front/**",
      "tests/**"
    ]
  }
}
